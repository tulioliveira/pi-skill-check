{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "mocha"
  ],
  "rules": {
    "strict": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "brace-style": ["error", "stroustrup"],
    "no-underscore-dangle": "off"
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  }
}
